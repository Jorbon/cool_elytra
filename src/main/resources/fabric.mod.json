{
	"schemaVersion": 1,
	"id": "cool_elytra",
	"version": "${version}",
	
	"name": "Cool Elytra",
	"description": "Rolls the camera while turning in flight",
	"authors": [
		"Jorbonism",
		"Motschen"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/cool-elytra-roll",
		"sources": "https://github.com/Jorbon/cool_elytra"
	},
	
	"license": "GPL-3.0",
	"icon": "assets/cool_elytra/icon.png",
	
	"environment": "client",
	"entrypoints": {
		"client": [
			"edu.jorbonism.cool_elytra.CoolElytraClient"
		]
	},
	"mixins": [
		{
			"config": "cool_elytra.mixins.json",
			"environment": "client"
		}
	],
	
	"depends": {
		"java": ">=${java_version}",
		"minecraft": ${minecraft_compatible_range},
		"fabricloader": "*",
		"fabric-api": "*",
		"midnightlib": "*"
	}
}
